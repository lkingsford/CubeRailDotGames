<html>

<head>
  <title>{{> title }}</title>
  <meta charset="utf-8" />
  <link rel="stylesheet" href="static/css/normalize.css">
  <link rel="stylesheet" href="static/css/skeleton.css">
  <link rel="stylesheet" href="static/css/cuberail.css">
  <link rel="icon" type="image/png" href="/static/favicon-16x16.png" sizes="16x16">
  <link rel="icon" type="image/png" href="/static/favicon-32x32.png" sizes="32x32">
  <link rel="icon" type="image/png" href="/static/favicon-96x96.png" sizes="96x96">
</head>

<body>
  <div class="container">
    <div class="row">
      <div class="twelve columns">
        <a href="/">
          <img src="static/img/logo1.svg" />
        </a>
      </div>
    </div>
    <div class="row">
      {{#if loggedin}}
      <div class="four columns">
        Logged in as {{username}}
      </div>
      {{else}}
      <div class="eight columns">
        <p />
      </div>
      <div class="two columns">
        <a href="newuser">Register</a>
      </div>
      <div class="two columns">
        <a href="login">Sign in</a>
      </div>
      {{/if}}
    </div>
    {{> content }}
  </div>
</body>

<script src="static/js/cuberail.js"></script>
<link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Cairo:wght@300&family=Cantora+One&display=swap" rel="stylesheet">

</html>